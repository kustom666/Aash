mash.app/Contents/PkgInfo: 
	@$(CHK_DIR_EXISTS) mash.app/Contents || $(MKDIR) mash.app/Contents 
	@$(DEL_FILE) mash.app/Contents/PkgInfo
	@echo "APPL????" >mash.app/Contents/PkgInfo
mash.app/Contents/Resources/empty.lproj: 
	@$(CHK_DIR_EXISTS) mash.app/Contents/Resources || $(MKDIR) mash.app/Contents/Resources 
	@touch mash.app/Contents/Resources/empty.lproj
	
mash.app/Contents/Info.plist: 
	@$(CHK_DIR_EXISTS) mash.app/Contents || $(MKDIR) mash.app/Contents 
	@$(DEL_FILE) mash.app/Contents/Info.plist
	@sed -e "s,@ICON@,TextEditor.icns,g" -e "s,@EXECUTABLE@,mash,g" -e "s,@TYPEINFO@,????,g" /usr/local/Qt4.5/mkspecs/macx-g++/Info.plist.app >mash.app/Contents/Info.plist
