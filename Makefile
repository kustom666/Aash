	@$(CHK_DIR_EXISTS) mash.app/Contents/Resources/ || $(MKDIR) mash.app/Contents/Resources/ 
	@$(DEL_FILE) mash.app/Contents/Resources/TextEditor.icns
	@$(COPY_FILE) TextEditor.icns mash.app/Contents/Resources/TextEditor.icns
